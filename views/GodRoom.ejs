<!DOCTYPE html>
<html lang="en">

<head>
   <meta charset="UTF-8">
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <title>Document</title>
   <script src="https://kit.fontawesome.com/3a67b0646e.js"></script>
   <link rel="stylesheet" href="/stylesheets/room.css">
   <script src="/socket.io/socket.io.js"></script>
   <script src="https://unpkg.com/peerjs@1.3.1/dist/peerjs.min.js"></script>
   <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
   <script>
      let roomId = "<%= roomId %>"
      let user = <%- JSON.stringify(user) %>;
      let room = <%- JSON.stringify(game) %>;
   </script>
</head>

<body>
   <section id="WaitingRoom">
      <div class="members">
         <ul id="memberlist">

         </ul>
         <button id="start" ><%= game.Members.length %> / <%= game.MaxMember %></button>
      </div>
      <div class="chat">
         <ul id="chatBox">
         </ul>
         <div class="send">
            <div class="inner">
               <input type="text" name="" id="msg" placeholder="message">
               <i class="bottom right"></i>
            </div>
            <button id="Send"><i class="fas fa-paper-plane"></i></button>
         </div>
      </div>
   </section>
   <section id="Room" class="hidden">
      <div class="left">
         <h1>God's Diary</h1>
         <ul id="Event">

         </ul>
         <div class="control">
            <button id="CR">Next Person</button>
         </div>
      </div>
      <div class="videoGrid" id="videoGrid">

      </div>
   </section>
   <script src="/javascripts/GodRoom.js"></script>
</body>

</html>